{
  "name": "huntX",
  "description": "Xpath-Finder",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "128" : "assets/icon/32.png"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": 
  [
    "contextMenus",
    "tabs",
    "activeTab",
    "webNavigation"
  ],
  "content_scripts":
   [
      {
        "matches":["<all_urls>"],
        "run_at":"document_start",
        "js": ["js/contentScript.js"]
      }
    ],
    "background":
    {
      "service_worker": "js/background.js",
      "type": "module"
    }
}